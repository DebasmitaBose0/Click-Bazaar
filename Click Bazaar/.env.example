# Environment Variables Template
# Copy this file to .env.local and fill in your values

# API Configuration
VITE_API_URL=http://localhost:5173
VITE_BACKEND_URL=http://localhost:3000

# Authentication
VITE_JWT_SECRET=your-secret-key-change-this-in-production

# Database (MongoDB)
VITE_MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/clickbazaar

# Google Gemini API (for AI Assistant)
VITE_GOOGLE_GEMINI_API_KEY=your-api-key-here

# Environment
VITE_NODE_ENV=development

# Deployment
VITE_VERCEL_URL=

# Feature Flags
VITE_ENABLE_LIVE_TRACKING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_EMAIL_NOTIFICATIONS=false

# Warehouse Configuration
VITE_WAREHOUSE_CITY=Barrackpore
VITE_WAREHOUSE_STATE=West Bengal
VITE_WAREHOUSE_PINCODE=700112
VITE_WAREHOUSE_LAT=22.7646
VITE_WAREHOUSE_LNG=88.2632

# Third-Party Services
VITE_SENTRY_DSN=
VITE_ANALYTICS_ID=
VITE_STRIPE_KEY=

# Timeouts
VITE_API_TIMEOUT=30000
VITE_SESSION_TIMEOUT=86400000
